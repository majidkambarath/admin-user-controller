   <!--End navbar-->
   
    <% if(details!=null) { %>
        <table class="table" id="example" >
              <thead>
                <tr>
                    <th>FirstName</th>
                    <th>LastName</th>
                    <th>Email</th>
                    <th>password</th>
                    <th>Actions</th>
                </tr>
              </thead>
                <tbody id="geeks">
                  <% details.forEach(function(userData){ %>
                    <tr>
                        <td><%= userData.firstname%></td>
                        <td><%= userData.lastname%></td>
                        <td><%= userData.email%></td>
                        <td><%= userData.password%></td>
                        <td>

                          <a href="/editUser?id=<%= userData._id%>" class="edit" title="Edit" data-toggle="tooltip"><i class="material-icons">&#xE254;</i></a>
                          <a href="/deleteUser?id=<%= userData._id%>" class="delete" title="Delete" data-toggle="tooltip"><i class="material-icons">&#xE872;</i></a>
                        </td>
                    </tr>
                <% }) %>
                </tbody>
        </table>
      <% } %>
       
   <div class="col-sm-4">
    <a href="/addUser"><button class="btn btn-info fa fa-plus">Add User</button></a>
</div>
   <script src="https://code.jquery.com/jquery-3.5.1.js"></script>
   <script src="https://cdn.datatables.net/1.13.1/js/jquery.dataTables.min.js"></script>
   <script src="https://cdn.datatables.net/1.13.1/js/dataTables.bootstrap5.min.js"></script>
   <script>
    $(document).ready(function () {
    $('#example').DataTable();
});
   </script>
</body>

